<img class="search-icon" src={{image}}>

<div class="search-details">
  <div class="search-text">
    <h2 class="search-text">{{searchText}}</h2>
    <h3 class="search-location">{{searchLocation}}</h3>
  </div>

  <div class="open-search">Open Search</div>
</div>

<div class="remove-search" {{action 'clickRemove'}}>
  {{#if clickRemove}}
    <img src={{icon-asset "searchSavedDelete.png"}}>
    Delete
  {{else}}
  {{/if}}
</div>
